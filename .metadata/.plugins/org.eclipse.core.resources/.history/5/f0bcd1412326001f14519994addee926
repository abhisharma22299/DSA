package Array;

public class DuplicateElement {
	public static void main(String[] args) {

		int arr[] = { 1, 2, 4, 5, 6, 7 };
		int arr2[]=new Array[];
		for (int i = 0; i < arr.length; i++) {

			arr2[arr[i]] += 1;
		}
	for(int i=0;i<arr2.length;i++) {
		if(arr[i]<=2) {
			System.out.println("Duplicate Element is  : " + arr[i] );
			return;
		}
	}

	}
}
